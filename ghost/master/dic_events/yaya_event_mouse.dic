    '\h\s[0]\u\s[16]ゴロゴロ…\w5…\w5。'
    '\h\s[0]\u\s[16]ん\w2～\w2～\w2…\w5…\w5。'
    '\h\s[0]\u\s[16]…\w5…\w5ん\w2～\w2な\w2ぅ\w2～\w2…\w5…\w5。'
}

MouseMove1Chin : nonoverlap {
    '\h\s[0]\u\s[16]ゴ\w2ロ\w2ゴ\w2ロ\w2ゴ\w2ロ\w2ゴ\w2ロ\w2…\w5…\w5。'
    '\h\s[0]\u\s[16]にゃぅ\w5にゃぅ\w5にゃぅ\w5にゃぅ\w5…\w5…\w5。'
    '\h\s[0]\u\s[16]そこそこ…\w5…\w5なぅ～～。'
    '\h\s[0]\u\s[10]んにゃ、\w5んにゃ、\w5んにゃ、\w5んにゃ…\w5…\w5。'
