Talk_eat_hungerfew : array {
    _eat = Meal_array('light')
    _meal = _eat[0]
    _cal = TOINT(_eat[1])

    '\u\s[10]\h\s[4]おなか減った…\w5…\w5。\w9\w9\u'
    '\u\s[10]\h\s[3]ちょっとお腹減ったな…\w5…\w5。\w9\w9\u'
    '\u\s[10]\h\s[34]何か軽く食べたいな。\w9\w9\u'
    '\u\s[10]\h\s[6]腹が減っては戦はできぬと申します。\w9\w9\u…\w5…\w5。\w9\w9\h\s[0]\n\n…\w5…\w5。\w9\w9\u\n\n'
    --
    if timesignal > 0 && (hour == 7 || hour == 11 || hour == 18) {
        '\s[16]もう少しで食事時なんだから、\w5少し待ったら？\w9\w9\h\s[4]\n\nえー。'
    }
    else {
        p0_s_fullness += _cal
        "%(_meal)でも食べたら？\w9\w9\h\s[6]\n\nん、\w5うまい。\w9\w9\u\s[11]\n\n早。"
        "\s[10]…\w5…\w5はい、\w5%(_meal)。\w9\w9\h\s[6]\n\nモグモグ…\w5…\w5。\w9\s[35]美味しい。"
    }
}

Talk_eat_hunger : array {
    _maindish  = Meal_array('maindish')
    _meal      = _maindish[0]
    _cal       = _maindish[1]
    p0_s_fullness += _cal
    --
    '\u\s[10]\h\s[4]おなか減った…\w5…\w5。\w9\w9'
    --
    '\u何か食べたら？\w9\w9'
    --
    "\h\s[5]\n\nそうだな。\w9\n%(_meal)でも作って食べよう。\w9\w9\u\s[11]\n\n作るて。\w9\w9"
    --
    "\h\s[6]\n\nんー、\w5空腹の時の%(_meal)は格別だな。\w9\w9"
    --
    "\u\s[11]\n\nもう食べてるし。"
}

Talk_eat_hungerover : array {
    _func  = GETFUNCLIST('Word_meal_')
    _meal  = ''
    _m_dm  = ('と','、','に、','と','、')
    _cal   = 0
    _flg   = 0
    _genre = ''
    _g_li  = (IARRAY)
    _g_dm  = '、\w5'

    // 食事リストの作成
    for _i = 0; _i < ARRAYSIZE(_func); _i ++ {
        // メインディッシュ以外
        if STRSTR(_func[_i], 'maindish', 0) == -1 {
            _t = Meal_array(_func[_i])
            _meal += _t[0]
            _meal += _m_dm[_i]
            _cal += TOINT(_t[1])
            _g_li ,= Meal_genre(_t[2])
        }
    }

    // genreが重複しているか判別して整形
    _genre = _g_li[0]
    for _i = 1; _i < ARRAYSIZE(_g_li); _i ++ {
        // 異なっている場合
        if _genre != _g_li[_i] {
            _genre = JOIN(_g_li, _g_dm)
            _flg = 1
            break
        }
    }

    p0_s_fullness += _cal

    {
        "\u\s[10]\h\s[4]おなか減って力が出ない…\w5…\w5。\w9\w9"
        --
        "\u\s[16]%(_meal)の用意ができてるわよ。\w9\w9"
        --
        if _flg == 1 {
            "\h\s[3]\n\n%(_genre)…\w5…\w5\s[4]バラバラだな。\w9\w9\u\s[11]\n\n文句言わない。\w9\w9"
        }
        else {
            "\h\s[5]\n\n今日は、\w5%(_genre)か。\w9\w9\u\s[10]\n"
        }
        --
        "\u\s[10]\nさあ、\w5たんとお食べ。\w9\w9"

        --
        "\h\s[6]\n\n…\w5…\w5実は用意したのは俺なんだが…\w5…\w5。\w9\s[0]ま！\w9いっか。\w9\n"
        "\h\s[6]\n\n"
        "\h\s[6]\n\n…\w5…\w5ああ。\w9\n"
        --

        "\h\s[6]いただきます。"
    }
}

