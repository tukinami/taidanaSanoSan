// ===================================================================
// 食事トーク
// ===================================================================
Talk_eat : array {
    _hunger_flag = On_Status.fullness.p0.judge

    _talk_funcs = IARRAY
    case _hunger_flag {
        when 2 {
            _talk_funcs ,= 'Talk_eat_fullover'
        }
        when 1 {
            _talk_funcs ,= 'Talk_eat_full'
        }
        when 0 {
            _talk_funcs ,= 'Talk_eat_normal'
        }
        when -1 {
            _talk_funcs ,= 'Talk_eat_hungerfew'
        }
        when -2 {
            _talk_funcs ,= 'Talk_eat_hunger'
        }
        others {
            _talk_funcs ,= 'Talk_eat_hungerover'
        }
    }

    _season = Time_month_toSeason
    if ISFUNC("Talk_eat_season_%(_season)") {
        _talk_funcs ,= "Talk_eat_season_%(_season)"
    }

    _talk_func = Array_toParallel(_talk_funcs)
    EVAL(_talk_func)
}

Talk_eat_fullover : array {
    '\u\s[10]\h\s[4]ちょっと食べすぎたな。\w9\w9\u少し普通の食事を減らそうか？\w9\w9\h\s[3]\n\nそれは嫌だ。\w9\w9\u\s[11]\n\n何故。'
    '\u\s[10]\h\s[34]う～、\w5ちょっと苦しい…\w5…\w5。\w9\w9\u食べすぎよ。\w9\w9\h\n\n…\w5…\w5\w9\s[0]人はこれを「幸福の苦しみ」と呼び\w9\w9\u\n\nません。'
    '\u\s[10]\h\s[34]う～ん、\w5食べ過ぎた…\w5…\w5。\w9\s[6]\nこういうときは、\w9\s[7]\n寝\w2る\w2に\w2限\w2り\w2ま\w2す\w2ね\w2！\w9\w9\u無理しないことは重要だけど、\w9\s[16]\n…\w5…\w5釈然としない！'
}

Talk_eat_full : array {
    '\u\s[10]\h\s[6]おなかがいっぱいで、\w5し\w2あ\w2わ\w2せ\w2…\w5…\w5。\w9\w9\u…\w5…\w5。\w9\w9\h\s[4]\n\n…\w5…\w5し\w2あ\w2わ\w5せ\w5で\w5眠\w5い\w5…\w5…\w5。\w9\w9\u\n\nだと思った。'
    '\u\s[10]\h\s[5]今日はたくさん美味しいものを食べました。\w9まる。\w9\w9\u…\w5…\w5。\w9\w9\h\s[6]\n\nよし、\w5これから、\w9\s[7]\n寝ていいですか！！\w9\w9\u\n\n駄目です。'
    '\u\s[10]\h\s[36]ふー、\w5食べた食べた。\w9\w9\uなら、\w5おやつは要らないわね。\w9\w9\h\s[2]\n\n！！\w9\n\w9\s[1]…\w5…\w5\w9\s[4]…\w5…\w5\w9\s[3]…\w5…\w5\w9\s[34]…\w5…\w5！！\w9\w9\u\n\n一人百面相やめい。'
}

Talk_eat_normal : array {
